<?xml version="1.0" encoding="utf-8"?>
<referencematerial xmlns="http://{{ url }}"
    xmlns:atom="http://www.w3.org/2005/atom">
    <publication>
        <atom:link rel="publication" type="application/xml" href="http://{{ url }}/publication/{{ referencematerial.publication.id }}"/>
    </publication>
    <name>
        {{ referencematerial.name }}
    </name>
    <url>
        {{ referencematerial.url }}
    </url>
    <notes>
        {{ referencematerial.notes }}
    </notes>
</referencematerial>
