<?xml version="1.0" encoding="utf-8"?>
<papergroups xmlns="http://{{ url }}"
    xmlns:atom="http://www.w3.org/2005/atom">
    {% for papergroup in papergroups %}
    <papergroup>
        <atom:link rel="papergroup" type="application/xml" href="http://{{ url }}/papergroup/{{ papergroup.id }}" />
    </papergroup>
    {% endfor %}
</papergroups>
