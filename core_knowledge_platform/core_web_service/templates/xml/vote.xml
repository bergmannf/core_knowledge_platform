<?xml version="1.0" encoding="utf-8"?>
<vote xmlns="http://{{ url }}"
    xmlns:atom="http://www.w3.org/2005/atom">
    <votetype>
        {{ vote.vote_type_string }}
    </votetype>
    <caster>
        <atom:link rel="user" type="application/xml" href="http://{{ url }}/user/{{ vote.caster.id }}"/>
    </caster>
    <comment>
        <atom:link rel="user" type="application/xml" href="http://{{ url }}/comment/{{ vote.comment.id }}"/>
    </comment>
</vote>
