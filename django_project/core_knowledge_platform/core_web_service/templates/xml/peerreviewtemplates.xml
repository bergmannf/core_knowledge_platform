<?xml version="1.0" encoding="utf-8"?>
<peerreviewtemplates xmlns="http://{{ url }}"
    xmlns:atom="http://www.w3.org/2005/atom">
    {% for peerreviewtemplate in peerreviewtemplates %}
    <peerreviewtemplate>
        <atom:link rel="peerreviewtemplate" type="application/xml" href="http://{{ url }}/peerreviewtemplate/{{ peerreviewtemplate.id }}"/>
    </peerreviewtemplate>
    {% endfor %}
</peerreviewtemplates>
